<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!-- 行程 -->
	<bean id="actDetail" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/_02_activity/actDetail.jsp"></property>
	</bean>

	<!-- 行程完成顯示頁 -->
	<bean id="display" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/_02_activity/display.jsp"></property>
	</bean>
	
	<!-- 顯示單一行程(有用到) -->
	<bean id="up_soloPage" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/_02_activity/soloPage_update.jsp"></property>
	</bean>		
	
	<bean id="sw_soloPage" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/_02_activity/solopage_show.jsp"></property>
	</bean>	
	
	<bean id="fav_display" class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="/_02_favorite/fav_display.jsp"></property>
		<property name="contextRelative" value="true"></property>
	</bean>
	
	
	
	
	
</beans>
